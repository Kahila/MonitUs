<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head') %>
</head>

<body class="page-header-fixed sidemenu-closed-hidelogo page-content-white page-md header-dark dark-sidebar-color logo-dark dark-theme">
    <div class="page-wrapper">
        <%- include('../partials/topbar'); %>
            <!-- start page container -->
            <div class="page-container">
                <!-- start sidebar menu -->
                <%- include('../partials/sidebar1'); %>
                    <!-- end sidebar menu -->
                    <div class="page-content-wrapper">
                        <div class="page-content">
                            <div class="page-bar">
                                <div class="page-title-breadcrumb">
                                    <div class=" pull-left">
                                        <div class="page-title">Student Marks</div>
                                    </div>
                                    <ol class="breadcrumb page-breadcrumb pull-right">
                                        <li><i class="fa fa-home"></i>&nbsp;<a class="parent-item" href="home">Home</a>&nbsp;<i class="fa fa-angle-right"></i>
                                        </li>
                                        <li class="active">Dashboard</li>
                                    </ol>
                                </div>
                            </div>
                            <!--  -->
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="tabbable-line">
                                        <div class="tab-content">
                                            <div class="tab-pane active fontawesome-demo" id="tab1">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="card">
                                                            
                                                            <div class="card-body ">
                                                                <div class="table-scrollable">
                                                                    <table style="border-spacing:0;"
                                                                        class="table table-striped  table-dark table-borderless table-hover table-checkable order-column valign-middle borderless"
                                                                        id="example4">
                                                                        <thead >
                                                                            <tr>
                                                                                <th>#</th>
                                                                                <th> Name </th>
                                                                                <th> Surname </th>
                                                                                <th> Grade </th>
                                                                                <th> Subject </th>
                                                                            </tr>
                                                                           
                                                                        </thead>
                                                                        <tbody>
                                                                            <% 
                                                                            var body = JSON.parse(students);
                                                                            var s = 0;
                                                                            for (var i = 0; i < body.length; i++){
                                                                                students = body[i].students.split(',');
                                                                                for (var j = 0; j < students.length; j++){
                                                                                    names = students[j].split(' ');
                                                                                    s++;
                                                                             %>
                                                                                <tr class="odd gradeX" onclick="window.location=`#`;">
                                                                                    <td>
                                                                                        <%= s %>
                                                                                    </td>
                                                                                    <td class="center"><%= names[0] %></td>
                                                                                    <td class="center"><%= names[1] %></td>
                                                                                    <td class="center"><%= body[i].grade %></td>
                                                                                    <td class="center"><%= body[i].subject %></td>
                                                                                    <!-- <td class="center">90%</td> -->
                                                                                    <!-- <td class="center">1</td> -->
                                                                                </tr>
                                                                                <%
                                                                                }
                                                                            }
                                                                                %>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                          
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--  -->
                        </div>
                        <!-- end page container -->
                        <!-- start footer -->
                        <%- include('../partials/footer'); %>
                            <!-- end footer -->
                    </div>
                    <%- include('../partials/scripts'); %>
</body>

</html>